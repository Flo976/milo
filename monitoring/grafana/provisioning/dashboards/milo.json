{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(milo_requests_total[5m])",
          "legendFormat": "{{endpoint}} {{method}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Latency p50 / p95 / p99",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(milo_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(milo_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(milo_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "GPU VRAM Usage",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "milo_gpu_vram_used_bytes / 1073741824",
          "legendFormat": "VRAM (GB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decgbytes",
          "min": 0,
          "max": 16,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 14 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Models Loaded",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "milo_models_loaded",
          "legendFormat": "Models"
        }
      ]
    },
    {
      "title": "Fallback Rate (Cloud -> Local)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(milo_llm_fallback_total[5m]) / rate(milo_llm_requests_total[5m])",
          "legendFormat": "Fallback %"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percentunit", "max": 1 },
        "overrides": []
      }
    },
    {
      "title": "Pipeline Stage Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(milo_pipeline_stage_duration_seconds_bucket[5m]))",
          "legendFormat": "{{stage}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Active Sessions",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "milo_active_sessions",
          "legendFormat": "Sessions"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(milo_requests_total{status=~\"4..|5..\"}[5m])",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["milo"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "title": "Milo Voice",
  "uid": "milo-voice",
  "version": 1
}
